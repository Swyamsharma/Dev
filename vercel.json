{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/vite",
      "config": { "distDir": "client/dist" }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}